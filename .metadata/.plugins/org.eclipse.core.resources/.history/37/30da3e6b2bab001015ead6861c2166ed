package com.klef.repository;
import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;
import com.klef.entity.SevaBooking;
@Repository

public interface SevaBookingRepository {


	

	
	public interface SevaBookingRepository extends JpaRepository<SevaBooking, Integer>
	{
	    SevaBooking findByEmail(String email);
	    SevaBooking findByContactNumber(String contactNumber);
	}



}
